{
	"name": "biblebowl/account",
	"description": "Bible Bowl management application",
	"keywords": ["bible bowl"],
	"type": "project",
    "license": "proprietary",
	"require": {
		"laravel/framework": "5.3.*",
		"thomaswelton/laravel-gravatar": "^1.0",
		"laravel/socialite": "^2.0",
		"illuminate/html": "5.*",
	  	"anhskohbo/no-captcha": "2.*",
	  	"silber/bouncer": "1.0.0-alpha.2",
	  	"alexpechkarev/google-geocoder": "dev-master",
		"jackpopp/geodistance": "^1.2",
		"ramsey/uuid": "3.*",
		"aws/aws-sdk-php": "~3.0",
		"landish/pagination": "~1.0",
		"bkuhl/easychimp": "0.*",
		"barryvdh/laravel-dompdf": "0.*",
		"anlutro/l4-settings": "0.*",
		"ignited/laravel-omnipay": "2.*",
		"omnipay/stripe": "2.*",
		"predis/predis": "1.*",
		"jenssegers/rollbar": "^1.5",
		"jmikola/geojson": "~1.0",
		"fzaninotto/faker": "^1.5"
	},
	"require-dev": {
		"symfony/dom-crawler": "~3.0",
		"symfony/css-selector": "~3.0",
	    "filp/whoops": "dev-master",
	  	"barryvdh/laravel-ide-helper": "2.*",
	  	"doctrine/dbal": "^2.5",
	  	"barryvdh/laravel-debugbar": "^2.0",
	  	"spatie/laravel-tail": "dev-master",
	  	"codeception/codeception": "~2.0",
	    "mockery/mockery": "dev-master"
	},
	"autoload": {
		"classmap": [
			"database"
		],
		"psr-4": {
			"BibleBowl\\": "app/"
		}
	},
	"autoload-dev": {
		"classmap": [
		  	"tests/Helpers/",
			"tests/TestCase.php"
		]
	},
	"scripts": {
		"post-root-package-install": [
			"php -r \"file_exists('.env') || copy('.env.example', '.env');\""
		],
		"post-create-project-cmd": [
			"php artisan key:generate"
		],
		"post-install-cmd": [
			"Illuminate\\Foundation\\ComposerScripts::postInstall",
			"php artisan optimize"
		],
		"post-update-cmd": [
			"Illuminate\\Foundation\\ComposerScripts::postUpdate",
			"php artisan optimize"
		]
	},
	"config": {
		"preferred-install": "dist"
	}
}
