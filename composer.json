{
	"name": "biblebowl/account",
	"description": "Bible Bowl management application",
	"keywords": ["bible bowl"],
	"type": "project",
    "license": "proprietary",
	"require": {
		"php": ">=7.0",
		"alexpechkarev/google-geocoder": "dev-master",
		"anhskohbo/no-captcha": "2.*",
		"anlutro/l4-settings": "0.*",
		"aws/aws-sdk-php": "~3.0",
		"barryvdh/laravel-dompdf": "0.*",
		"bkuhl/easychimp": "~1.0",
		"doctrine/dbal": "^2.5",
		"fzaninotto/faker": "^1.5",
		"ignited/laravel-omnipay": "2.*",
		"jackpopp/geodistance": "^1.2",
		"jenssegers/rollbar": "^1.5",
		"jmikola/geojson": "~1.0",
		"landish/pagination": "~1.0",
		"laravel/framework": "5.4.*",
		"laravel/socialite": "^3.0",
		"laravel/tinker": "~1.0",
		"laravelcollective/html": "5.*",
		"league/html-to-markdown": "^4.4",
		"maatwebsite/excel": "~2.1.0",
		"omnipay/stripe": "2.*",
		"predis/predis": "1.*",
		"ramsey/uuid": "3.*",
		"silber/bouncer": "1.0.0-beta.2",
		"spatie/laravel-backup": "^4.7",
		"thomaswelton/laravel-gravatar": "^1.0",
		"league/flysystem-aws-s3-v3": "^1.0"
	},
	"require-dev": {
		"phpunit/phpunit": "5.*",
		"symfony/css-selector": "3.2.*",
		"symfony/dom-crawler": "3.2.*",
	    "filp/whoops": "dev-master",
	  	"barryvdh/laravel-ide-helper": "2.*",
	  	"barryvdh/laravel-debugbar": "^2.0",
	  	"spatie/laravel-tail": "dev-master",
	    "mockery/mockery": "0.*",
		"laravel/browser-kit-testing": "1.*"
	},
	"autoload": {
		"classmap": [
			"database",
			"tests"
		],
		"psr-4": {
			"App\\": "app/"
		}
	},
	"autoload-dev": {
		"classmap": [
		  	"tests/Helpers/",
			"tests/TestCase.php"
		]
	},
	"scripts": {
		"test": [
			"phpunit -d memory_limit=512M"
		],
		"post-root-package-install": [
			"php -r \"file_exists('.env') || copy('.env.example', '.env');\""
		],
		"post-create-project-cmd": [
			"php artisan key:generate"
		],
		"post-install-cmd": [
			"Illuminate\\Foundation\\ComposerScripts::postInstall",
			"php artisan optimize"
		],
		"post-update-cmd": [
			"Illuminate\\Foundation\\ComposerScripts::postUpdate",
			"php artisan optimize"
		]
	},
	"config": {
		"preferred-install": "dist",
		"sort-packages": true
	}
}
